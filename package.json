{
  "name": "boilerplate",
  "version": "1.0.0",
  "description": "boilerplate-api",
  "type": "module",
  "author": "≈Åukasz Klejszta",
  "private": true,
  "license": "UNLICENSED",
  "engines": {
    "node": "==18"
  },
  "scripts": {
    "prepare": "husky",
    "start": "node dist/startup.js",
    "build": "rm -rf dist && node_modules/.bin/tsc --project tsconfig.build.json && node_modules/.bin/tsc-alias \"./dist\"",
    "kill-app": "pkill -f \"npm run watch\" & pkill -f \"npm run test-watch\" & true",
    "run-in-docker": "APP_ENV=docker docker-compose -f docker-compose.yml -f docker-compose.app.yml run ${DOCKER_COMPOSE_RUN_ARGS:---interactive --tty} app sh -c \"$npm_config_command\"",
    "watch": "COMMAND=\"node_modules/.bin/nodemon -e ts --exec \\\"sh -c 'npm run build && APP_ENV=${npm_config_env:-docker} npm run start'\\\"\" ./scripts/run-app.sh",
    "migration-run": "COMMAND='npm run build && ./node_modules/.bin/ts-node ./dist/migrate.js' ./scripts/run-app.sh | sed 's|file://app/||g'",
    "eslint": "COMMAND='./node_modules/.bin/eslint ./src ./test --format ./eslint.formatter.mjs' ./scripts/run-app.sh",
    "eslint-fix": "COMMAND='./node_modules/.bin/eslint . --fix' ./scripts/run-app.sh",
    "types": "COMMAND=\"node_modules/.bin/tsc --project tsconfig.json --noEmit\" ./scripts/run-app.sh",
    "integration": "DIRECTORY=test CONFIG_PATH=vitest.config.integration.ts ./scripts/run-test.sh",
    "units": "DIRECTORY=src CONFIG_PATH=vitest.config.ts ./scripts/run-test.sh",
    "open-api-docs": "OPEN=${npm_config_open:-true} ./scripts/generate-open-api-docs.sh",
    "open-api-docs-generate-test-data": "export OPEN_API_PATH=./temp/$(date +%FT%T%Z) && npm run open-api-docs && rm ./src/shared/lib/open-api-docs-generator.lib.test.zip && zip -r './src/shared/lib/open-api-docs-generator.lib.test.zip' './temp/openapi.yaml' \"$OPEN_API_PATH\""
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.817.0",
    "@aws-sdk/lib-dynamodb": "^3.817.0",
    "@types/express-serve-static-core": "5.0.6",
    "awilix": "^10.0.2",
    "axios": "^1.9.0",
    "body-parser": "^1.19.0",
    "dotenv": "^6.0.0",
    "express": "^4.16.3",
    "express-async-errors": "^3.1.1",
    "generic-interceptor": "^2.2.1",
    "http-method-enum": "^1.0.0",
    "http-status-codes": "^2.3.0",
    "ioredis": "^5.6.1",
    "kysely": "^0.28.2",
    "luxon": "^3.6.1",
    "mongodb": "^6.16.0",
    "mysql2": "^3.14.1",
    "nock": "^14.0.4",
    "path-to-regexp": "^8.2.0",
    "pg": "^8.16.0",
    "qs": "^6.9.4",
    "url-parse": "^1.5.10",
    "uuid": "^11.1.0",
    "winston": "^3.17.0",
    "zod": "^3.24.3"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@eslint/js": "^9.27.0",
    "@ngneat/falso": "^7.3.0",
    "@redocly/cli": "^1.34.3",
    "@types/adm-zip": "^0.5.7",
    "@types/dotenv": "^4.0.3",
    "@types/express": "4.17.0",
    "@types/luxon": "^3.6.2",
    "@types/node": "^18.19.108",
    "@types/pg": "^8.15.2",
    "@types/qs": "^6.5.3",
    "@types/supertest": "^6.0.3",
    "@types/to-json-schema": "^0.2.4",
    "@types/url-parse": "^1.4.11",
    "@types/uuid": "^3.4.4",
    "@types/winston": "^2.3.7",
    "@typescript-eslint/parser": "^8.33.0",
    "@vitest/ui": "^3.1.4",
    "adm-zip": "^0.5.16",
    "cockatiel": "^3.2.1",
    "eslint-config-prettier": "^10.1.5",
    "eslint-formatter-stylish": "^8.40.0",
    "eslint-import-resolver-node": "^0.3.9",
    "eslint-import-resolver-typescript": "^4.4.1",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-import-esm": "^2.0.0",
    "eslint-plugin-import-x": "^4.13.3",
    "eslint-plugin-prettier": "^5.4.0",
    "eslint-plugin-unused-imports": "^4.1.4",
    "husky": "^9.1.7",
    "jiti": "^2.4.2",
    "nodemon": "^3.1.10",
    "open": "^10.1.2",
    "open-cli": "^8.0.0",
    "openapi-types": "^12.1.3",
    "prettier": "^3.5.3",
    "supertest": "^7.1.1",
    "to-json-schema": "^0.2.5",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.16",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.33.0",
    "vitest": "^3.1.4"
  }
}
